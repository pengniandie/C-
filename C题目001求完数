//一个数如果恰好等于它的因子之和，这个数就称为"完数"。例如6=1＋2＋3.编程找出1000以内的所有完数。
#include<stdio.h>
int fun (int m)
{
    int sum = 0;
    for(int n=1; n<m; n++)
    {
         if(n % m == 0)
         {
              sum = sum + n;
         }
    }
    if(sum == m)
    {
         return 1;
    }
    else 
    {
        return 0;
    }
}

int main()
{
    int fun (int m);
    int s;
    for(int s =1; s<=1000; s++)
   {
        if(fun(s))
      {
           printf("%d\n"); 
      }
   }
}
